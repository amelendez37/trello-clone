{
    "name": "trello-clone",
    "version": "1.0.0",
    "description": "Track projects and tasks",
    "main": "index.js",
    "scripts": {
        "start:server": "nodemon index.js",
        "posttest": "eslint --ignore-path .gitignore .",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/amelendez37/trello-clone.git"
    },
    "author": "Aaron Melendez",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/amelendez37/trello-clone/issues"
    },
    "homepage": "https://github.com/amelendez37/trello-clone#readme",
    "dependencies": {
        "axios": "^0.18.0",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "joi": "^13.6.0",
        "mongodb": "^3.1.3",
        "mongoose": "^5.2.9",
        "winston": "^3.0.0"
    },
    "devDependencies": {
        "eslint": "^5.3.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-react": "^7.11.1",
        "jest": "^23.5.0",
        "nodemon": "^1.18.3"
    },
    "jest": {
        "moduleNameMapper": {
